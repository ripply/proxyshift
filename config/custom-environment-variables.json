{
    "push": {
        "gcm": {
            // contains actual api key
            "serverapikey": "PUSH_GCM_KEY"
        },
        "apn": {
            // contains actual certificate or path to it
            // defaults to cert.pem
            "cert": "PUSH_APN_CERT",
            // contains actual key or path to is
            // defaults to key.pem
            "key": "PUSH_APN_KEY"
        },
        "wns": {
            // contains actual client id
            "id": "PUSH_WNS_ID",
            // contains actual client secret
            "secret": "PUSH_WNS_SECRET"
        }
    },
    "google": {
        "api": {
            "gmail": {
                "user": "GMAIL_USER",
                "client_id": "GMAIL_CLIENT_ID",
                "client_secret": "GMAIL_CLIENT_SECRET",
                "refresh_token": "GMAIL_REFRESH_TOKEN",
                "access_token": "GMAIL_ACCESS_TOKEN",
                "pass": "GMAIL_PASS"
            }
        }
    },
    "sendgrid": {
        "api": {
            "key": "SENDGRID_API_KEY"
        },
        "templates": {
            "failed_group_activation": "SENDGRID_TEMPLATE_FAILED_GROUP_ACTIVATION",
            "successful_group_activation": "SENDGRID_TEMPLATE_SUCCESSFUL_GROUP_ACTIVATION",
            "added_to_group": "SENDGRID_TEMPLATE_ADDED_TO_GROUP",
            "removed_from_group": "SENDGRID_TEMPLATE_REMOVED_FROM_GROUP",
            "password_reset": "SENDGRID_TEMPLATE_PASSWORD_RESET",
            "permission_level_decrease": "SENDGRID_TEMPLATE_PERMISSION_LEVEL_DECREASE",
            "permission_level_increase": "SENDGRID_TEMPLATE_PERMISSION_LEVEL_INCREASE",
            "failed_account_activation": "SENDGRID_TEMPLATE_FAILED_ACCOUNT_ACTIVATION",
            "successful_account_activation": "SENDGRID_TEMPLATE_SUCCESSFUL_ACCOUNT_ACTIVATION",
            "email_verification": "SENDGRID_TEMPLATE_EMAIL_VERIFICATION",
            "account_invitation": "SENDGRID_TEMPLATE_ACCOUNT_INVITATION",
            "create_group_invitation": "SENDGRID_TEMPLATE_CREATE_GROUP_INVITATION"
        }
    },
    "email": {
        "transactional": {
            "address": "TRANSACTIONAL_EMAIL",

        },
        "contactus": {
            "address": "CONTACTUS_EMAIL"
        }
    },
    "web": {
        "url": "WEB_URL"
    },
    "rabbit": {
        "url": "CLOUDAMQP_URL",
        "workers": "RABBIT_WORKERS"
    },
    "email": {
        "interval": {
            "reset": "EMAIL_INTERVAL_RESET"
        }
    },
    "slack": {
        "webhook": "SLACK_WEBHOOK",
        "bot": {
            "token": "SLACK_BOT_TOKEN",
            "name": "SLACK_BOT_NAME",
            "channel": {
                "election": "SLACK_BOT_CHANNEL_ELECTION",
                "control": "SLACK_BOT_CHANNEL_CONTROL"
            }
        }
    },
    "tokens": {
        "expires": "TOKENS_EXPIRE_IN_X_SECONDS",
        "refresh": "TOKENS_REFRESH_AFTER_X_SECONDS"
    },
    "periodic": {
        "interval": "PERIODICALLY_CLEANUP_EVERY_X_SECONDS"
    }
}
