{
    "push": {
        "gcm": {
            // contains actual api key
            "serverapikey": "PUSH_GCM_KEY"
        },
        "apn": {
            // contains actual certificate or path to it
            // defaults to cert.pem
            "cert": "PUSH_APN_CERT",
            // contains actual key or path to is
            // defaults to key.pem
            "key": "PUSH_APN_KEY"
        },
        "wns": {
            // contains actual client id
            "id": "PUSH_WNS_ID",
            // contains actual client secret
            "secret": "PUSH_WNS_SECRET"
        }
    },
    "google": {
        "api": {
            "gmail": {
                "user": "GMAIL_USER",
                "client_id": "GMAIL_CLIENT_ID",
                "client_secret": "GMAIL_CLIENT_SECRET",
                "refresh_token": "GMAIL_REFRESH_TOKEN",
                "access_token": "GMAIL_ACCESS_TOKEN",
                "pass": "GMAIL_PASS"
            }
        }
    },
    "sendgrid": {
        "api": {
            "key": "SENDGRID_API_KEY"
        }
    },
    "email": {
        "transactional": {
            "address": "TRANSACTIONAL_EMAIL",
            "templates": {
                "group_invitation": "SENDGRID_EMAIL_TEMPLATE_GROUP_INVITATION",
                "password_reset": "SENDGRID_EMAIL_TEMPLATE_PASSWORD_RESET",
                "verify_email": "SENDGRID_EMAIL_TEMPLATE_VERIFY_EMAIL",
                "account_activated": "SENDGRID_EMAIL_TEMPLATE_ACCOUNT_ACTIVATED",
            }
        },
        "contactus": {
            "address": "CONTACTUS_EMAIL"
        }
    },
    "web": {
        "url": "WEB_URL"
    },
    "rabbit": {
        "url": "CLOUDAMQP_URL",
        "workers": "RABBIT_WORKERS"
    },
    "email": {
        "interval": {
            "reset": "EMAIL_INTERVAL_RESET"
        }
    },
    "slack": {
        "webhook": "SLACK_WEBHOOK"
    },
    "tokens": {
        "expires": "TOKENS_EXPIRE_IN_X_SECONDS",
        "refresh": "TOKENS_REFRESH_AFTER_X_SECONDS"
    },
    "periodic": {
        "interval": "PERIODICALLY_CLEANUP_EVERY_X_SECONDS"
    }
}
